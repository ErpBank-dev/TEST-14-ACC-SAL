        <template id="pos_order_receipt_pdf_template_doc">
            <t t-call="web.basic_layout">
                <t t-foreach="docs" t-as="o">
                    <t t-set="o" t-value="o.with_context(lang='en_US')"/>
                    <div class="page" style="font-size:30px; ">
                        <div style="text-align:center;">
                            <t t-if='o.company_id.logo'>
                                <img t-att-src="'/logo.png?company=%s' % (o.company_id.id or 0)"
                                     style="padding: 0px; margin: 0px; height: auto; max-width: 200px; max-height: 100px;"
                                     t-att-alt="'%s' % o.company_id.name"/>
                            </t>

                        </div>

                    </div>
                </t>


                <center>
                    <span t-set="company" t-value="o.company_id"/>
                    <div style="margin:0 auto;padding:0 auto;white-space:nowrap; margin-top: 1pt">
                        <div>
                            <span t-esc="company.name"/>
                            <br/>
                            <t t-if="company.phone">
                                Phone:
                                <span t-esc="company.phone"/>
                                <br/>
                            </t>
                            <t t-if="company.street">

                                <span t-esc="company.street"/>
                                <br/>
                            </t>
                            <t t-if="company.website">

                                <span t-esc="company.website"/>
                                <br/>
                            </t>
                            <t t-if="company.vat">
                                TAX ID:
                                <span t-esc="company.vat"/>
                                <br/>
                            </t>
                            <div>----------------------------------</div>
                        </div>
                    </div>
                </center>

                <div class="page" style="font-size:10px;">

                    <br></br>
                    <div style="width:47%;float:left;white-space:nowrap">
                        <table style="line-height:5pt">
                            <tr>
                                <td>
                                    <b>Date:</b>
                                </td>
                                <td>
                                    <b>
                                        <span t-field="o.invoice_date"/>
                                    </b>
                                </td>
                                <td>
                                    <b>:التاريخ</b>
                                </td>
                            </tr>
                        </table>


                    </div>
                    <br/>
                    <!--                    <br/>-->
                    <div style="width:53%;float:left; white-space:nowrap">

                        <b>Invoice NO:</b>
                        <span t-field="o.name"/>
                        <b>:رقم الفاتورة</b>
                    </div>
                    <br style="line-height:5pt"/>
                    <!--                    <br/>-->

                    <!--                    <span t-set="company" t-value="o.company_id"/>-->
                    <!--                    <div style="margin:0 auto;padding:0 auto;white-space:nowrap;float:left">-->
                    <div>
                        <div style="width:80%; font-size:10px">
                            <t t-if="o.partner_id">
                                <table>
                                    <tr style="width:50%; font-size:10px">
                                        <td>
                                            <b>Customer:</b>

                                        </td>
                                        <td>
                                            <span t-field="o.partner_id" style="white-space:nowrap;font-size:10px"/>
                                        </td>
                                        <td>
                                            <b>:العميل</b>
                                        </td>
                                    </tr>
                                </table>


                            </t>
                            <!--                            <br/>-->
                            <t t-if="o.partner_id.vat">
                                <b>VAT NO:</b>
                                <span t-field="o.partner_id.vat"/>
                                <b>:الرقم الضريبي</b>
                                <br/>
                            </t>
                            <!--                            <t t-if="o.branch_id">-->
                            <!--                                <table>-->
                            <!--                                    <tr>-->
                            <!--                                        <td>Branch:</td>-->
                            <!--                                        <td><span t-field="o.branch_id.name"/></td>-->
                            <!--                                        <td>:الفرع</td>-->
                            <!--                                    </tr>-->
                            <!--                                </table>-->
                            <!--                            </t>-->
                        </div>
                    </div>
                    <br/>
                    <br/>

                    <table width="100%"
                           style="border-collapse:separate;border-spacing:0 4px;">
                        <tr style="text-decoration:underline ; font-size: 2 pt">
                            <th>ITEM
                                <br/>
                                الصنف
                            </th>
                            <th class="text-right">Qty<br/>الكميه
                            </th>
                            <th class="text-right">price
                                <br/>
                                السعر
                            </th>
                            <th class="text-center">subtotal
                                <br/>
                                الاجمالي
                            </th>
                        </tr>
                        <tr t-foreach="o.invoice_line_ids" t-as="line">
                            <b>
                            <td style="width:30%; font-size:8px; white-space:nowrap">
                                <span t-field="line.with_context(lang='ar_001').product_id.name"
                                      t-options="{'widget': 'text'}"/>
                                <div t-if="line.discount > 0">
                                    <t t-if="line.product_id.name != line.with_context(lang='ar_001').name">
                                        <br/>
                                        <span t-field="line.with_context(lang='ar_001').name"
                                              t-options="{'widget': 'text'}"/>

                                    </t>
                                </div>
                            </td>
                            </b>
                            <!--                                                        <t t-if="not line.display_type" name="account_invoice_line_accountable">-->
                            <!--                                                                <td name="account_invoice_line_name">-->
                            <!--                                                                    <span t-field="line.product_id.name" t-options="{'widget': 'text'}"/>-->
                            <!--                                                                    <t t-if="line.product_id.name != line.with_context(lang='ar_001').product_id.name">-->
                            <!--                                                                        <br/>-->
                            <!--                                                                        <span t-field="line.with_context(lang='ar_001').product_id.name" t-options="{'widget': 'text'}"/>-->
                            <!--                                                                    </t>-->
                            <!--                                                                </td>-->
                            <b>
                            <td class="text-right" style="width:22%; font-size:9px; white-space:nowrap">
                                <span t-field="line.quantity"/>
                                <span t-field="line.product_id.uom_id"/>
                            </td>
                            </b>
                            <b>
                            <td class="text-right" style="width:20%; font-size:10px">
                                <span t-field="line.price_unit"
                                      t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/>
                            </td>
                            </b>
                            <b>
                            <td class="text-center" style="width:28%; font-size:10px">
                                <span t-field="line.price_subtotal"
                                      t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/>
                            </td>
                            </b>
                        </tr>
                        <t t-if="o.return_invoice_id">
                            <tr t-foreach="o.return_invoice_id.invoice_line_ids" t-as="line">
                            <b>
                            <td style="width:30%; font-size:8px; white-space:nowrap">
                                <span t-field="line.with_context(lang='ar_001').product_id.name"
                                      t-options="{'widget': 'text'}"/>
                                <div t-if="line.discount > 0">
                                    <t t-if="line.product_id.name != line.with_context(lang='ar_001').name">
                                        <br/>
                                        <span t-field="line.with_context(lang='ar_001').name"
                                              t-options="{'widget': 'text'}"/>

                                    </t>
                                </div>
                            </td>
                            </b>
                            <b>
                            <td class="text-right" style="width:22%; font-size:9px; white-space:nowrap">
                                <span t-esc="line.quantity*-1"/>
                                <span t-field="line.product_id.uom_id"/>
                            </td>
                            </b>
                            <b>
                            <td class="text-right" style="width:20%; font-size:10px">
                                <span t-field="line.price_unit"
                                      t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/>
                            </td>
                            </b>
                            <b>
                            <td class="text-center" style="width:28%; font-size:10px">
                                <span t-esc="line.price_subtotal*-1"
                                      t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/>
                            </td>
                            </b>
                        </tr>

                        </t>

                    </table>
                     <div class="col-xs-12">
                                    <span>
                                        <b>Amount Sales:</b>
                                    </span>

                                    <b><span t-esc="round(o.amount_untaxed,2)" /></b>
                                    <b><span t-esc="o.currency_id.symbol" /></b>
                                    <span>
                                        <b>المبلغ المباع</b>
                                    </span>
                        </div>
                        <div class="col-xs-12">
                                    <span>
                                        <b>Amount Return:</b>
                                    </span>

                                    <b><span t-esc="round(o.return_invoice_id.amount_untaxed if o.return_invoice_id else 0,2)" /></b>
                                    <b><span t-esc="o.currency_id.symbol" /></b>
                                    <span>
                                        <b>المبلغ المردود</b>
                                    </span>
                        </div>
                    <table width="95%"
                           style="border-collapse:separate;border-spacing:0px;margin-top:4px;">
                        <tr style="font-size:17px">
                            <td style="width:75%; white-space:nowrap; font-size:10px">Subtotal:</td>
                            <td style="width:25%; font-size:10px" class="text-center">
                                <span t-esc="o.amount_untaxed"
                                      t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/>
                            </td>
                            <td style="white-space:nowrap; font-size:10px">:الاجمالي قبل الضريبة</td>
                        </tr>
                        <!--                        <tr t-foreach="o.get_order_tax_value_in_receipt()"-->
                        <!--                            t-as="taxdetail">-->
                        <!--                            <td>-->
                        <!--                                <span t-esc="taxdetail"/>-->
                        <!--                            </td>-->
                        <!--                            <td class="text-right">-->
                        <!--                                <span t-esc="taxdetail_value"-->
                        <!--                                      t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/>-->
                        <!--                            </td>-->
                        <!--                        </tr>-->
                        <tr t-if="o.get_total_discount_in_receipt() > 0.0">
                            <td>Discount:</td>
                            <td class="text-right">
                                <span t-esc="o.get_total_discount_in_receipt()"
                                      t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/>
                            </td>
                        </tr>

                        <tr style="font-size:17px; width=95%">
                            <td style="white-space:nowrap; font-size:10px">Vat Amount:</td>
                            <td class="text-center" style="white-space:nowrap; font-size:10px">
                                <span t-field="o.amount_tax"
                                      t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/>
                            </td>
                            <td style="white-space:nowrap; font-size:10px ">:مبلغ الضريبة</td>
                        </tr>
                        <tr style="font-size:17px; white-space:nowrap; width=95%">
                            <td style="white-space:nowrap; font-size:10px">Net Amount:</td>
                            <td class="text-center" style="white-space:nowrap; font-size:10px">
                                <span t-field="o.amount_total"
                                      t-options="{'widget': 'monetary', 'display_currency': o.currency_id}"/>
                            </td>
                            <td style="white-space:nowrap; font-size:10px">:الاجمالي شامل الضريبة</td>
                        </tr>
                        <tr>
                            <td colspan="2">
                                <!--                                <strong>Amount in Words:</strong>-->
                                <!--                                <br/>-->
                                <!--                                <span t-esc="o.config_id.currency_id.amount_to_text(o.amount_total)"/>-->
                                <!--                                <span t-esc="o._amount_to_words(o.amount_total)"/>-->
                                <!--                                <br/>-->
                                <b>
                                    <span t-esc="o._arabic_amount_to_words(o.amount_total)"></span>
                                </b>
