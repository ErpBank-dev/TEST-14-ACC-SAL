	<record id="invoice_receipt_pdf" model="ir.actions.report">
		<field name="name">POS Receipt</field>
		<field name="report_type">qweb-pdf</field>
		<field name="report_name">eq_pos_receipt_backend.invoice_receipt_pdf_template</field>
		<field name="report_file">eq_pos_receipt_backend.invoice_receipt_pdf_template</field>
		<field name="model">account.move</field>
		<field name="binding_type">report</field>
	</record>

	<record id="paperformat_pos_pdf_report" model="report.paperformat">
		<field name="name">PoS Receipt - PDF</field>
		<field name="default" eval="True" />
		<field name="format">custom</field>
		<field name="page_height">250</field>
		<field name="page_width">85</field>
		<field name="orientation">Portrait</field>
		<field name="margin_top">0</field>
		<field name="margin_bottom">1</field>
		<field name="margin_left">4</field>
		<field name="margin_right">4</field>
		<field name="header_line" eval="False" />
		<field name="header_spacing">50</field>
		<field name="dpi">90</field>
		<field name="report_ids"
			eval="[(6,0,[ref('eq_pos_receipt_backend.invoice_receipt_pdf'),])]" />
	</record>

</odoo>