<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_stock_move_receipt_order_negative_form" model="ir.ui.view">
        <field name="name">stock.move.negative.form</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='group_id']" position="after">
                <field name="sale_return_line_id"/>
                <field name="created_return_sale_line_id"/>
            </xpath>
        </field>
    </record>

</odoo>
