<odoo>
    <data>

        <record id="partner_category" model="ir.module.category">
            <field name="name">Partner</field>
            <field name="parent_id" ref="base.module_category_accounting"/>


        </record>

        <record id="partner_user" model="res.groups">
            <field name="name">User</field>
            <field name="category_id" ref="partner_category"/>

        </record>

        <record id="partner_manager" model="res.groups">
            <field name="name">Manager</field>
            <field name="category_id" ref="partner_category"/>

        </record>

        <record id="partner_rule_personal" model="ir.rule">
            <field name="name">Personal Partner</field>
            <field ref="base.model_res_partner" name="model_id"/>
            <field name="domain_force">['|',('user_id','=',user.id),('user_id','=',False)]</field>
            <field name="groups" eval="[(4, ref('journal_partner_access.partner_user'))]"/>
        </record>

        <record id="partner_rule_manager" model="ir.rule">
            <field name="name">Manager Partner</field>
            <field ref="base.model_res_partner" name="model_id"/>
            <field name="domain_force">[(1,'=',1)]</field>
            <field name="groups" eval="[(4, ref('journal_partner_access.partner_manager'))]"/>
        </record>


    </data>
</odoo>